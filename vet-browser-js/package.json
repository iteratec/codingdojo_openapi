{
  "name": "vet-browser-js",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=16",
    "npm": ">=8.19"
  },
  "scripts": {
    "predev": "npm run build:openapi",
    "dev": "vite",
    "prebuild": "npm run build:openapi",
    "build": "run-p type-check build-only",
    "prepreview": "npm run build:openapi",
    "preview": "vite preview --port 4173",
    "pretest:unit": "npm run build:openapi",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build:openapi:example": "rm -rf ./src/generated-sources-openapi/example && openapi-generator-cli generate -i https://petstore3.swagger.io/api/v3/openapi.json -g typescript-fetch -o  ./src/generated-sources-openapi/example --additional-properties=supportsES6=true,npmVersion=8.19.2,typescriptThreePlus=true",
    "build:openapi:vet-health": "rm -rf ./src/generated-sources-openapi/vet-health && openapi-generator-cli generate -i ../vet-health-golang/health-check-api.yaml -g typescript-fetch -o  ./src/generated-sources-openapi/vet-health --additional-properties=supportsES6=true,npmVersion=8.19.2,typescriptThreePlus=true",
    "build:openapi:vet-home": "rm -rf ./src/generated-sources-openapi/vet-home && openapi-generator-cli generate -i ../vet-home-spring/home-api.yaml -g typescript-fetch -o ./src/generated-sources-openapi/vet-home --additional-properties=supportsES6=true,npmVersion=8.19.2,typescriptThreePlus=true",
    "build:openapi": "npm run build:openapi:example && npm run build:openapi:vet-health && npm run build:openapi:vet-home"
  },
  "dependencies": {
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.23.0",
    "vue-tsc": "^0.40.7"
  }
}
